{
  "cSpell.words": [
    "exchangee"
  ],
  "vscodeGitCommit.template": [
    "{tag}: {page}/{otherTarget}→{title}",
    "",
    "{message}"
  ],
  "vscodeGitCommit.variablesDisplayTitles": {
    "scope": "Fill the pseudo of the person commiting with an @"
  },
  "vscodeGitCommit.variables": {
    "prefix": "keke",
    "tag": [
      {
        "label": "feat",
        "detail": "新增/修改功能 (feature)"
      },
      {
        "label": "fix",
        "detail": "修補 bug (bug fix)"
      },
      {
        "label": "docs",
        "detail": "文件 (documentation)"
      },
      {
        "label": "style",
        "detail": "格式 (不影響程式碼運行的變動 white-space, formatting, missing semi colons, etc)"
      },
      {
        "label": "refactor",
        "detail": " 重構 (既不是新增功能，也不是修補 bug 的程式碼變動)。"
      },
      {
        "label": "chore",
        "detail": "增加或修改第三方套件(輔助工具)等 (maintain)"
      },
      {
        "label": "deploy",
        "detail": "部屬配置"
      }
    ],
    "author": [
      {
        "label": "古家寧",
        "detail": "輸入開發者"
      }
    ],
    "page": [
      {
        "label": "showdownGame"
      },
      {
        "label": "player"
      },
      {
        "label": "hand"
      },
      {
        "label": "deck"
      },
      {
        "label": "index"
      },
      {
        "label": "card"
      },
      {
        "label": "round"
      },
      {
        "label": "vs code"
      },
      {
        "label": "開發用套件"
      },
      {
        "label": "API Service"
      },
      {
        "label": "SCSS"
      },
      {
        "label": "共用組件"
      },
      {
        "label": "共用 Service"
      },
      {
        "label": "設定頁面"
      },
      {
        "label": "路由"
      },
      {
        "label": "core module"
      },
      {
        "label": "app module"
      },
      {
        "label": "環境變量"
      },
      {
        "label": "其他"
      },
      {
        "label": "開發環境設定"
      }
    ]
  }
}